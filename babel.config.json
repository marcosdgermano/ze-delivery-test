{
  "presets": [
    "@babel/preset-react", 
    [
      "@babel/preset-env", {
        "targets": {
          "esmodules": true
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-react-jsx", 
    [
      "babel-plugin-styled-components",
      {
        "displayName": true,
        "fileName": false
      }
    ]
  ]
}